---
permalink: /site.webmanifest
eleventyExcludeFromCollections: true
---
{ "name": "{{ metadata.title | escape }}", "short_name": "{{ metadata.title | escape }}",
    "start_url": "/",
    "scope": "/",
    "icons": [
        {
            "src": "/android-chrome-192x192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "/android-chrome-512x512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "/maskable_icon.png",
            "sizes": "640x640",
            "type": "image/png",
            "purpose": "any maskable"
        }
    ],
    "theme_color": "#0867e2",
    "background_color": "#0867e2",
    "display": "standalone",
    "share_target": {
        "action": "/",
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    }
}